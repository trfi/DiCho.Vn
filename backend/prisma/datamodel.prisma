type User {
  id: ID! @id
  phone: String! @unique
  phoneVerified: Boolean @default(value: false)
  email: String
  emailVerified: Boolean @default(value: false)
  name: String
  username: String
  gender: [Gender]
  password: String!
  birthday: DateTime
  address: String
  permission: [Permission]
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}
enum Gender {
    M
    F
    O
}
enum Permission {
    ADMIN
    MODERATOR
    USER
}